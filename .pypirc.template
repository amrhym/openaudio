# PyPI configuration template
# Copy this to ~/.pypirc and update with your credentials

[distutils]
index-servers =
    pypi
    testpypi

[pypi]
repository = https://upload.pypi.org/legacy/
username = __token__
password = # Your PyPI API token here (starts with pypi-)

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = # Your TestPyPI API token here (starts with pypi-)